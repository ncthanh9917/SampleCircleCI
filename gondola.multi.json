{
    "tests": "./built/**/*_test.js",
    "timeout": 10000,
    "output": "./results",
    "helpers": {
        "WebDriver": {
            "url": "https://demo.gondolatest.com",
            "browser": "chrome",
            "windowSize": "maximize",            
            "restart": true,
            "keepBrowserState": false,
            "keepCookies": false,
            "waitForTimeout": 10000,
            "smartWait": 5000,
            "timeouts": {
                "script": 60000,
                "pageLoad": 10000
            }
        },
        "Mochawesome": {}
    },
    "include": {},
    "bootstrap": false,
    "plugins": {
        "screenshotOnFail": {
            "enabled": true
        },
        "wdio": {
            "services": [
                "selenium-standalone"
            ],
            "seleniumArgs": {
                "javaArgs": ["-Dwebdriver.edge.driver=F:/softwares/webdrivers/edgedriver_85/msedgedriver.exe"]
            },
            "enabled": true
        }
    },
    "mocha": {
        "reporterOptions": {
            "reportDir": "results/Multi",
            "inlineAssets": true,
            "timestamp": "isoDateTime",
            "reportFilename": "[Multi]demo_report",
            "autoOpen": true,
            "reportPageTitle": "[Multi]demo_report"
        }
    },
    "name": "ts_example_multi",
    "multiple": {
        "regression": {
            "chunks": 2,
            "browsers": [
                "chrome",
                "firefox"
            ]
        }
    }
}