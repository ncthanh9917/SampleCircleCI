{
    "tests": "./built/**/*_test.js",
    "timeout": 10000,
    "output": "./results",
    "helpers": {
        "Appium": {
          "url": "https://demo.gondolatest.com",
          "require": "gondolajs/built/helper/AppiumEx",
          "smartWait": 5000,
          "platform": "Android",
          "restart": false,
          "browser": "chrome",
          "port": 4723,
          "timeouts": {
            "script": 60000,
            "page load": 30000
          },
          "desiredCapabilities": {
            "automationName": "UiAutomator2",
            "newCommandTimeout": 600,
            "autoGrantPermissions": true,
            "noReset": true,
            "adbExecTimeout": 120000,
            "browserName": "chrome",
            "udid": "emulator-5554",
            "nativeWebScreenshot": true,
            "deviceName": "emulator",
            "platformName": "android"
          }
        },
        "Mochawesome": {}
      },
    "include": {},
    "bootstrap": false,
    "plugins": {
        "screenshotOnFail": {
            "enabled": true
        },
        "wdio": {
            "services": [
                "selenium-standalone"
            ],
            "enabled": true
        }
    },
    "mocha": {
        "reporterOptions": {
            "reportDir": "results/MobileWeb",
            "inlineAssets": true,
            "timestamp": "isoDateTime",
            "reportFilename": "[MobileWeb]GeneralMills_demo_report",
            "autoOpen": true,
            "reportPageTitle": "[MobileWeb]GeneralMills_demo_report"
        }
    },
    "name": "ts_example_mobile_web"
}